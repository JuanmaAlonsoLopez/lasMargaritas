{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: none",
          "-moz-appearance",
          "backdrop-filter"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "typescript-config/is-valid": "off"
  }
}